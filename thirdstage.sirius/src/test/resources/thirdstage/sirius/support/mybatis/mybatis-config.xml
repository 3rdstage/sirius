<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="${settings.cacheEnabled}"/>
		<setting name="lazyLoadingEnabled" value="${settings.lazyLoadingEnabled}"/>
		<setting name="aggressiveLazyLoading" value="${settings.aggressiveLazyLoading}"/>
		<setting name="defaultExecutorType" value="${settings.defaultExecutorType}"/>
		<setting name="defaultStatementTimeout" value="${settings.defaultStatementTimeout}"/>
	</settings>
	
</configuration>